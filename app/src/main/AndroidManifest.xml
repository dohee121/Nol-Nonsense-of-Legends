<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.nol">
    <!-- 진동 사용권한 획득 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <!-- 화면 밝기 조정 사용권한 획득 -->
    <uses-permission android:name="android.permission.WRITE_SETTINGS"
        tools:ignore="ProtectedPermissions" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_nol"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_nol_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Nol">

        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".A0_Enter" />
        <activity android:name=".A1_Rabbit" />
        <activity android:name=".A2_Egg" />
        <activity android:name=".A3_Birthday" />
        <activity android:name=".A4_Pizza" />
        <activity android:name=".A5_Owl"/>

        <activity android:name=".Drink"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".Base" />
    </application>

</manifest>